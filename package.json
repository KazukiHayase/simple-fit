    "expo-dev-client": "^3.3.7",
    "expo-linking": "^6.3.1",
    "expo-router": "^3.5.16",
    "expo-status-bar": "^1.12.1",
    "react": "18.2.0",
    "react-hook-form": "^7.50.1",

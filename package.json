{
  "name": "simple-fit",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc",
    "ci": "biome lint . && tsc",
    "check": "biome check --apply .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": ["biome check --apply"]
  },
  "dependencies": {
    "@realm/react": "^0.6.2",
    "dayjs": "^1.11.10",
    "expo": "^50.0.4",
    "expo-constants": "~15.4.5",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.6",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-paper": "^5.11.5",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "realm": "^12.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@biomejs/biome": "1.4.1",
    "@types/react": "~18.2.14",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
